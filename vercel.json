{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "cleanUrls": true,
    "trailingSlash": false,
    "images": {
        "sizes": [320, 640, 768, 828, 1024, 1280, 1536],
        "minimumCacheTTL": 2678400
    },
    "crons": [
        {
            "path": "/api/admin/job/report",
            "schedule": "0 22 * * *"
        },
        {
            "path": "/api/admin/job/cleanup",
            "schedule": "0 22 * * *"
        }
    ],
    "redirects": [
        {
            "source": "/edit",
            "destination": "/compose",
            "permanent": true
        }
    ]
}
